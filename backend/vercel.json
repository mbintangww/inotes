{
    "version": 1,
    "builds": [
        {
            "src": "*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/notes",
            "dest": "server.js"
        },
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}